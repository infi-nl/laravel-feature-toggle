    "description": "Laravel 4 package wrapping joshuaestes/feature-toggle",
    "license": "MIT",
    "keywords": [
        "laravel", "feature", "toggle"
    ],
    "authors": [
        {
            "name": "Ely Deckers",
            "email": "ely@infi.nl"
        }
    ],
    "require": {
